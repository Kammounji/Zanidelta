{% extends "ReclamationDresseurReclamationBundle:Default:layout.html.twig" %}
        {% block head %}
            {{ parent() }}
        {% endblock %}
    {% block content %}

        <div class="cp-wrapper">

        {% block header %}
            {{ parent() }}
        {% endblock %}


        <section class="cp-departments-section cp-departments-section_v2 pd-t100">
            <div class="cp-bg-movement" data-ibg-bg="images/banner/White_screen.jpg"></div>
            <div class="container" >
                <div class="row">
                    <div class="col-md-6" >
                        <div class="cp-heading-style1_v1">
                            <h2>Declarer animal perdu</h2>
                        </div>
                        <form class="cp-checkout-form" method="post" action="http://html.crunchpress.com/pet-care/form4.php">
                            <div class="row">

                                <div class="col-md-6 col-sm-12">
                                    <div class="inner-holder">
                                        <label>ID</label>
                                        <input type="text" name="id" >
                                    </div>
                                </div>

                                <div class="col-md-6 col-sm-12">
                                    <div class="inner-holder">
                                        <label>ID_animal</label>
                                        <input type="text" name="id_animal" >
                                    </div>
                                </div>

                                <div class="col-md-6 col-sm-12">
                                    <div class="inner-holder">
                                        <label>date_disparition</label>
                                        <input type="date" name="date">
                                    </div>
                                </div>

                                <div class="col-md-12 col-sm-12">
                                    <div class="inner-holder">
                                        <label>lieu_disparition</label>
                                        <input type="text" name="lieu" >
                                    </div>
                                </div>

                                <div class="col-md-12 col-sm-12">
                                    <div class="inner-holder">
                                        <label>etat</label>
                                        <input type="text" name="etat" >
                                    </div>
                                </div>


                                <div class="col-md-12 col-sm-12">
                                    <div class="input-field btn-holder">
                                        <button type="submit" class="cp-btn-style1 btn-submit" value="Submit" name="submit">Soumettre</button>
                                    </div>
                                </div>

                            </div>
                        </form>
                    </div>

                    <div class="col-md-6">
                        <iframe width="600" height="450" frameborder="0" style="border:0"
                                src="https://www.google.com/maps/embed/v1/place?q=place_id:ChIJQ_V-Xn8z_RIRdQJKcU6ScdY&key=AIzaSyBExXPRcPTN3kFVW0ViJlvogBXDNQkcDPY" allowfullscreen></iframe>
                    </div>

                </div>
            </div>
        </section>



        <div class="cp-main-content">

            <section class="cp-departments-section pd-t100">
                <div class="cp-bg-movement" data-ibg-bg="images/banner/banner-before-img.png"></div>
                <div class="container">


                    <div class="row">
                        <div class="col-md-6">

                            <div class="cp-heading-style1_v2">
                                <h2>Espace Magasin</h2>

                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-6">

                            <div class="cp-accordian-item">
                                <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
                                    <div class="panel panel-default">
                                        <div class="panel-heading" role="tab" id="cp-headingOne">
                                            <h3 class="panel-title">
                                                <a role="button" data-toggle="collapse" data-parent="#accordion" href="#cp-collapseOne" aria-expanded="true" aria-controls="cp-collapseOne">
                                                    <span class="icon-medical micon"></span>
                                                    Ajouter store
                                                </a>
                                            </h3>
                                        </div>
                                        <div id="cp-collapseOne" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="cp-headingOne">
                                            <div class="panel-body">
                                                <div class="cp-text">
                                                    <p>ici vous pouvez ajouter et vendre vos produits sur notre store </p>
                                                </div>
                                                <button type="submit" class="btn-submit cp-btn-style2" value="Submit" name="submit">Ajouter</button>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="panel panel-default">
                                        <div class="panel-heading bg1" role="tab" id="cp-headingTwo">
                                            <h3 class="panel-title">
                                                <a class="collapsed" role="button" data-toggle="collapse" data-parent="#accordion" href="#cp-collapseTwo" aria-expanded="false" aria-controls="cp-collapseTwo">
                                                    <span class="icon-medical32 micon"></span>
                                                    modifier/supprimer store
                                                </a>
                                            </h3>
                                        </div>
                                        <div id="cp-collapseTwo" class="panel-collapse collapse" role="tabpanel" aria-labelledby="cp-headingTwo">
                                            <div class="panel-body">
                                                <div class="cp-text">
                                                    <p>ici vous modifier le prix,la quantite,la description ou le poid de vos produits ou de les enlever du store </p>
                                                </div>
                                                <button type="submit" class="btn-submit cp-btn-style2" value="Submit" name="submit">Modifier/Supprimer</button>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="panel panel-default">
                                        <div class="panel-heading" role="tab" id="cp-headingThree">
                                            <h3 class="panel-title">
                                                <a class="collapsed" role="button" data-toggle="collapse" data-parent="#accordion" href="#cp-collapseThree" aria-expanded="false" aria-controls="cp-collapseThree">
                                                    <span class="icon-medical1 micon"></span>
                                                    Ajouter encheres
                                                </a>
                                            </h3>
                                        </div>
                                        <div id="cp-collapseThree" class="panel-collapse collapse" role="tabpanel" aria-labelledby="cp-headingThree">
                                            <div class="panel-body">
                                                <div class="cp-text">
                                                    <p>ici vous pouvez ajouter et vendre vos produits sur notre store encheres </p>
                                                </div>
                                                <button type="submit" class="btn-submit cp-btn-style2" value="Submit" name="submit">Ajouter</button>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="panel panel-default">
                                        <div class="panel-heading bg2" role="tab" id="cp-headingFour">
                                            <h3 class="panel-title">
                                                <a class="collapsed" role="button" data-toggle="collapse" data-parent="#accordion" href="#cp-collapseFour" aria-expanded="false" aria-controls="cp-collapseFour">
                                                    <span class="icon-medical2 micon"></span>
                                                    modifier/supprimer encheres
                                                </a>
                                            </h3>
                                        </div>
                                        <div id="cp-collapseFour" class="panel-collapse collapse" role="tabpanel" aria-labelledby="cp-headingFour">
                                            <div class="panel-body">
                                                <div class="cp-text">
                                                    <p>ici vous modifier la date du debut d'un evennement d'encheres,le seuil de mise ou la description de vos produits ou de les enlever du store encheres </p>
                                                </div>
                                                <button type="submit" class="btn-submit cp-btn-style2" value="Submit" name="submit">Modifier/Supprimer</button>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="panel panel-default">
                                        <div class="panel-heading bg3" role="tab" id="cp-headingFive">
                                            <h3 class="panel-title">
                                                <a class="collapsed" role="button" data-toggle="collapse" data-parent="#accordion" href="#cp-collapseFive" aria-expanded="false" aria-controls="cp-collapseFive">
                                                    <span class="icon-medical32 micon"></span>
                                                    deplacer vos produits
                                                </a>
                                            </h3>
                                        </div>
                                        <div id="cp-collapseFive" class="panel-collapse collapse" role="tabpanel" aria-labelledby="cp-headingFive">
                                            <div class="panel-body">
                                                <div class="cp-text">
                                                    <p>ici vous pouvez deplacer vos produits du store vers le store encheres ou bien le contraire </p>
                                                </div>
                                                <button type="submit" class="btn-submit cp-btn-style2" value="Submit" name="submit">Deplacer</button>
                                            </div>
                                        </div>

                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6">

                            <figure class="cp-thumb">
                                <img src="{{ asset('images/magasin.jpg') }}" alt="">
                            </figure>
                        </div>
                    </div>
                </div>
            </section>
            {% block footer %}
                {{ parent() }}
            {% endblock %}
        </div>

        {% block script %}
            {{ parent() }}
        {% endblock %}
    {% endblock %}